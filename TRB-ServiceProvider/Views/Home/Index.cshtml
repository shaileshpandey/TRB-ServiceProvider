@{
    ViewBag.Title = "Service Provider";
}

@section featured {
<section class="featured">
    <div class="content-wrapper">
        <hgroup class="title">
            <h1>@ViewBag.Title.</h1>
            <h2>@ViewBag.Message</h2>
        </hgroup>
    </div>
</section>
}
@{Html.BeginForm("Index", "Home");}
  @Html.DropDownList("user", (List<SelectListItem>)ViewBag.Users)
  <input type="submit" value="Initiate SSO"/>
@{Html.EndForm();}